    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "cwd": "${workspaceRoot}",
            "executable": "build/NRF_Robot.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "serverpath": "C:/Program Files (x86)/SEGGER/JLink/JLinkGDBServerCL.exe",
            "device": "nRF52832_xxAA",
            // "serialNumber": "59104218", //if you have more than one J-Link probe add the serial number here
            "serialNumber": "788529266", //if you have more than one J-Link probe add the serial number here
            "svdFile": "${workspaceRoot}/nrf52.svd",
            "runToMain": true,
        },
        {
            "name": "Attach",
            "cwd": "${workspaceRoot}",
            "executable": "build/NRF_Robot.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "jlink",
            "serverpath": "C:/Program Files (x86)/SEGGER/JLink/JLinkGDBServerCL.exe",
            "device": "nRF52832_xxAA",
            // "serialNumber": "59104218", //if you have more than one J-Link probe add the serial number here
            "serialNumber": "788529266", //if you have more than one J-Link probe add the serial number here
            "svdFile": "${workspaceRoot}/nrf52.svd",
        }
    ]
}